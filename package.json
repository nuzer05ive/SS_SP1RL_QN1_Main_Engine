{
  "name": "sp1rl-rh-portal",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint || true",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit",
    "e2e": "playwright test",
    "print:pdf": "node scripts/print-pdf.js",
    "paper": "npm run build && (\n  PORT=$( (lsof -ti :3000 >/dev/null && echo 4000) || echo 3000 )\n  echo \"ðŸŒ Using PORT=$PORT\"\n  PAPER_URL=http://localhost:$PORT/docs/paper PORT=$PORT npm start & echo $! > .server.pid\n  npx wait-on http://localhost:$PORT\n  PAPER_URL=http://localhost:$PORT/docs/paper node scripts/print-pdf.js\n  code=$?\n  kill $(cat .server.pid) 2>/dev/null || true\n  rm -f .server.pid\n  exit $code\n)",
    "freeport": "kill -9 $(lsof -ti :3000) 2>/dev/null || true"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.2.0",
    "katex": "^0.16.10",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-latex-next": "^2.2.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "husky": "^8.0.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}